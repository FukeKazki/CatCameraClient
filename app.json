{
	"expo": {
		"name": "cat-camera",
		"slug": "cat-camera",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#FDF0DE"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.develop.app"
		},
		"android": {
			"package": "com.develop.app",
			"versionCode": 1,
			"permissions": []
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
